endTimeHNX2 = 15*60;
PhienSangHNX = (endTimeHNX1 - startTimeHNX1);
PhienChieuHNX = (endTimeHNX2 - startTimeHNX2);
FulltimeHNX = (endTimeHNX1 - startTimeHNX1) + (endTimeHNX2 - startTimeHNX2);
